  },
  "mixins": {
    "FireLoop": true,
    "Timestamp": true,
    "Owner": true
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "href": {
      "type": "string"
    },
    "isDeleted": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "createdAt": {
      "type": "date",
      "required": false
    },
    "updatedAt": {
      "type": "date",
      "required": false
